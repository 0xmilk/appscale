   <div id="header">
        <div class="navbar">
            <div class="navbar-inner">
              <div class="container-fluid">
                <a class="brand" href="dashboard.html">AppScale.<span class="slogan">PaaS</span></a>
                <div class="nav-no-collapse">
                    <ul style="padding-right: 2.5%;" class="nav pull-right usernav">
                        <% if logged_in? %>
                          <h3>User: <%=h user_email %> |
                          <%= link_to "Logout", :controller => :users, :action => :logout %></h3><br />
                        <% else %>
                          <h3><%= link_to "Login", :controller => :users, :action => :login %> |     
                          <%= link_to "Create Account", :controller => :users, :action => :new %></h3>
                        <% end %>
                    </ul><!--close drop down-->

                </div><!-- /.nav-collapse -->
              </div>
            </div><!-- /navbar-inner -->
          </div><!-- /navbar --> 

    </div><!-- End #header -->

    <div id="wrapper">

        <!--Responsive navigation button-->  
        <div class="resBtn">
            <a href="#"><span class="icon16 minia-icon-list-3"></span></a>
        </div>
        
        <!--Left Sidebar collapse button-->  
        <div class="collapseBtn leftbar">
             <a href="#" class="tipR" title="Hide Left Sidebar"><span class="icon12 minia-icon-layout"></span></a>
        </div>

        <!--Sidebar background-->
        <div id="sidebarbg"></div>
        <!--Sidebar content-->
        <div id="sidebar">

            <div class="shortcuts">
                <ul>
                    <li><a href="http://appscale.com" title="Support" class="tip"><span class="icon24 icomoon-icon-support"></span></a></li>
                </ul>
            </div><!-- End search -->            

            <div class="sidenav">

                <div class="sidebar-widget" style="width: 210px; margin: -1px 0 0 0;">
                    <h5 class="title" align="center" style="margin-bottom:0">Navigation</h5>
                </div><!-- End .sidenav-widget -->

                <div align="center" class="mainnav">
                    <ul>
                            <li align="center" class="tab <%= highlight_active 'status' %>"><%= link_to "Cloud Status", :controller => :status, :action => :cloud %></li>

                            <% if is_user_cloud_admin %>
                              <li align="center" class="tab <%= highlight_active 'authorize' %>"><%= link_to "Authorize User", :controller => :authorize, :action => :cloud %></li>
                            <% end %>
                            <% if i_can_has_upload? %>
                              <li align="center" class="tab <%= highlight_active 'upload' %>"><%= link_to "Upload App", :controller => :apps, :action => :new %></li>
                              <li align="center" class="tab <%= highlight_active 'delete' %>"><%= link_to "Delete App", :controller => :apps, :action => :delete %></li>
                            <% end %>

                            

                    </ul>
                </div>
            </div><!-- End sidenav -->

        </div><!-- End #sidebar -->
</div> <!--close wwrapper-->
